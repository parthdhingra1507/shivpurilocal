{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app.py"
        },
        {
            "src": "/admin",
            "dest": "/admin.html"
        },
        {
            "src": "/panel",
            "dest": "/panel.html"
        },
        {
            "src": "/transport",
            "dest": "/transport.html"
        },
        {
            "src": "/places",
            "dest": "/places.html"
        },
        {
            "src": "/food",
            "dest": "/food.html"
        },
        {
            "src": "/news",
            "dest": "/news.html"
        },
        {
            "src": "/forum",
            "dest": "/forum.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}